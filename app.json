{
  "expo": {
    "name": "react-native-app",
    "slug": "react-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./appImage/splashScreenIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./appImage/splashScreenIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aspatel.reactnativeapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "SEND_SMS"
      ],
      "package": "com.aspatel.reactnativeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "82cc6ac4-09f9-48c6-a6d0-d659491abaf7"
      }
    }
  }
}
