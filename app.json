{
  "expo": {
    "name": "work-cost-estimator",
    "slug": "react-native-app",   // This will configure with expo app name
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splashScreenIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aspatel.reactnativeapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIcon.png",   // to show app icon
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "SEND_SMS",
      ],
      "package": "com.aspatel.reactnativeapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "82cc6ac4-09f9-48c6-a6d0-d659491abaf7"
      }
    }
  }
}
